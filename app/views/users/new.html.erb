<% semantic_form_for @user do |form| %>
  <%= hidden_field_tag :return_to, params[:return_to] %>

  <%= form.error_messages %>

  <%= form.inputs :screen_name, :email, :password, :password_confirmation %>

  <% form.buttons do %>
    <%= form.commit_button :label => 'Sign up' %>
  <% end %>
<% end %>
